{
  "name": "sjs",
  "version": "1.0.0",
  "description": "A JS Library with special attention to Data Types.",
  "main": "sjs.js",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "webpack-scaffolding": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "chai": "^4.0.2",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^0.4.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "html-webpack-plugin": "^2.29.0",
    "jsdoc": "^3.5.4",
    "jsdom": "11.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^3.4.2",
    "mocha-silent-reporter": "^1.0.0",
    "surge": "^0.19.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register --require babel-polyfill",
    "test:silent": "mocha -R mocha-silent-reporter --compilers js:babel-register --require babel-polyfill",
    "test:watch": "webpack-dev-server --hot --config webpack.mocha.js",
    "dev": "webpack-dev-server --port 3000 --inline --colors --watch --hot --progress",
    "preprod": "npm run test:silent",
    "prod": "NODE_ENV='production' webpack --progress",
    "dox": "doxdox 'src/*.js' --layout markdown --output DOCUMENTATION.md",
    "generate-docs": "node_modules/.bin/jsdoc --readme docs/assets/HOMEDOCS.md -c jsdoc.json",
    "postgenerate-docs": "npm run dox",
    "deployAPI": "surge docs/ api.sjs.surge.sh"
  },
  "author": "Gabriele Romeo",
  "license": "ISC"
}
